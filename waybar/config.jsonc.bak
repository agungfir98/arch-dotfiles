[
  //------------------------------------------------------------------------------------
  //    Laptop Bar
  //------------------------------------------------------------------------------------
  {
    //--------------------------------------------------------------------------------
    //    General
    //--------------------------------------------------------------------------------
    "output": "eDP-1",
    "layer": "top",
    "position": "top",
    "height": 34,
    "spacing": 0,
    "margin-top": 5,
    "margin-left": 100,
    "margin-bottom": 0,
    "margin-right": 100,
    //--------------------------------------------------------------------------------
    //    Left Modules
    //--------------------------------------------------------------------------------
    "modules-left": [
      "clock",
      "hyprland/workspaces"
    ],
    //--------------------------------------------------------------------------------
    //    Center Modules
    //--------------------------------------------------------------------------------
    "modules-center": [],
    //--------------------------------------------------------------------------------
    //    Right Modules
    //--------------------------------------------------------------------------------
    "modules-right": [
      "custom/spotify",
      "wireplumber",
      "bluetooth",
      "network",
      "cpu",
      "memory",
      "battery"
    ],
    //--------------------------------------------------------------------------------
    //    Left Configuration
    //--------------------------------------------------------------------------------
    "hyprland/workspaces": {
      "disable-scroll": true,
      "disable-select-unfocused": true,
      "all-outputs": true,
      "warp-on-scroll": false,
      "format": "{icon}",
      "format-icons": {
        "1": "Ë®à", //calculate :3
        "2": "‰∫å",
        "3": "‰∏â",
        "4": "Âõõ",
        "5": "‰∫î",
        "6": "ÂÖ≠",
        "7": "‰∏É",
        "8": "ÂÖ´",
        "9": "‰πù",
        "10": "‰πù"
      }
    },
    "clock": {
      "format": " {:%a /%d /%m  ‚îÇ  %I:%M} ",
      "tooltip-format": "{:%B of %Y}",
      "on-click": "galendae"
    },
    //--------------------------------------------------------------------------------
    //    Right Configuration
    //--------------------------------------------------------------------------------
    "bluetooth": {
      "format": "Û∞Ç≤",
      "format-on": "Ôäî",
      "format-connected": "Ôäî {num_connections}",
      "tooltip-format": "{status}",
      "tooltip-format-on": "{status}",
      "tooltip-format-connected": "{device_alias}",
      "on-click": "kitty --class bluetui -e bluetui",
      "interval": 1
    },
    "memory": {
      "interval": 1,
      "format": "{}% Ôî∏"
    },
    "cpu": {
      "format": "{usage}% Ôãõ",
      "interval": 1
    },
    "battery": {
      "interval": 1,
      "states": {
        "good": 90,
        "warning": 15,
        "critical": 5
      },
      "format": "{capacity} {icon}",
      "format-full": "{capacity} {icon}",
      "format-charging": "{capacity} Ôá¶",
      "format-plugged": "{capacity} Ôá¶",
      "format-icons": [
        "ÔâÑ",
        "ÔâÉ",
        "ÔâÇ",
        "ÔâÅ",
        "ÔâÄ"
      ]
    },
    "network": {
      "format-wifi": "Ôá´",
      "format-disconnected": "‚ö†",
      "on-click": "kitty --class nmtui -e nmtui",
      "tooltip-format": "{essid}"
    },
    "wireplumber": {
      "format": "{volume} {icon}",
      "format-muted": " Ôö© ",
      "format-icons": {
        "default": [
          " ÔÄ¶",
          " ÔÄß",
          " ÔÄ®"
        ]
      },
      "on-click-right": "pavucontrol",
      "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle"
    },
    "custom/spotify": {
      "exec": "/sbin/python3 ~/.config/waybar/mediaplayer.py --player spotify",
      "format": "{}  ÔÜº",
      "return-type": "json",
      "on-click": "playerctl --player=spotify play-pause",
      "on-double-click": "playerctl --player=spotify next",
      "on-double-click-right": "playerctl --player=spotify previous"
    }
  },
